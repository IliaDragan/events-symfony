{% extends('IPGEventsBundle::layout.html.twig') %}
{% block content %}
    Event name: {{  event.name }}
    <br />
    Event location: {{ event.locationId }}
    <br />
    Event description: {{ event.description }}
    <br />
    Pictures:
    {% for picture in pictures %}
        <img src="/{{ picture.webPath }}" alt=""/>
    {% endfor %}
{% endblock %}
